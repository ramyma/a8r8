{
  "name": "ex_sd",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --config ./vitest.config.ts",
    "build": "tsc && vite build --config ./vitest.config.ts",
    "preview": "vite preview",
    "test": "vitest --config ./vitest.config.ts",
    "coverage": "vitest run --coverage",
    "jest": "jest --watch-all"
  },
  "eslintConfig": {
    "settings": {
      "react": {
        "version": "detect"
      },
      "import/resolver": {
        "typescript": {}
      },
      "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module"
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:import/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:react-hooks/recommended"
    ],
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "transform": {
      "node_modules/.pnpm/(react-konva|konva)/.+\\.(j|t)sx?$": "ts-jest",
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/.pnpm/(?!(react-konva|konva)/)"
    ],
    "testEnvironment": "jsdom"
  },
